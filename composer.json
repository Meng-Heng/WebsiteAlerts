{
    "name": "mengheng/website-alerts",
    "description": "This is an extension to Parsedown, to add support for the alerts styling for web pages.",
    "keywords": [
        "parsedown",
        "alerts",
        "gfm-alerts"
    ],
    "homepage": "https://github.com/Meng-Heng/WebsiteAlerts",
    "license": "MIT",
    "authors": [
        {
            "name": "Meng-Heng",
            "email": "havmengheng111@gmail.com",
            "homepage": "https://github.com/Meng-Heng",
            "role": "Developer"
        }
    ],
    "require": {
        "erusev/parsedown": "^1.7",
        "php": "^7.1||^8.0"
    },
    "autoload": {
        "psr-4": {
            "MengHeng\\ParsedownAlerts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.23",
        "phpunit/phpunit": "^10.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
